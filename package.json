{
  "name": "cbb-api",
  "version": "1.6.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/CFBD/cbb-api.git"
  },
  "private": true,
  "main": "src/app.ts",
  "scripts": {
    "build": "tsoa spec-and-routes && tsc --skipLibCheck",
    "build:db": "yarn kysely-codegen --camel-case --out-file=./src/config/types/db.d.ts",
    "build:authdb": "yarn kysely-codegen --camel-case --out-file=./src/config/types/authdb.d.ts",
    "dev": "concurrently \"nodemon\" \"nodemon -x tsoa spec-and-routes\"",
    "start": "node ./build/src/app.js",
    "test": "jest",
    "lint": "eslint  --ignore-path .gitignore .",
    "lint:fix": "eslint --ignore-path .gitignore . --fix",
    "prettier-check": "prettier -c --ignore-path .gitignore \"**/*.{css,html,js,json,md,yaml,yml,ts}\"",
    "prettier-check:updated": "pipe-git-updated --base=main --ext=css --ext=html --ext=js --ext=ts --ext=json --ext=md --ext=yaml --ext=yml -ext=js --ext=mdx -- prettier -c",
    "prettify": "prettier --write --ignore-path .gitignore \"**/*.{css,html,js,json,yaml,yml,ts,tsx,jsx,vue,}\" -c",
    "prepare": "husky"
  },
  "keywords": [
    "basketball",
    "college",
    "cbb",
    "ncaab",
    "data",
    "statistics"
  ],
  "author": "BlueSCar",
  "license": "MIT",
  "dependencies": {
    "@tsoa/runtime": "^6.6.0",
    "body-parser": "^1.20.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "helmet": "^8.0.0",
    "kysely": "^0.27.5",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.2",
    "swagger-ui-express": "^5.0.1",
    "tsoa": "^6.6.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@commitlint/types": "^19.5.0",
    "@jest-mock/express": "^2.1.0",
    "@jest/globals": "^29.7.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/body-parser": "^1.19.5",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.10",
    "@types/pg": "^8.11.10",
    "@types/swagger-ui-express": "^4.1.7",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "babel-jest": "^29.7.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.18.0",
    "eslint-plugin-prettier": "^5.2.3",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "joi": "^17.13.3",
    "kysely-codegen": "^0.17.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "semantic-release": "^24.2.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3"
  }
}
